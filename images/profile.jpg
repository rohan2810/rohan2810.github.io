<svg xmlns="http://www.w3.org/2000/svg" width="200" height="200" viewBox="0 0 200 200">
  <rect width="200" height="200" fill="#1a1a1a" stroke="#00ff00" stroke-width="3"/>
  
  <!-- Retro pixelated person -->
  <rect x="80" y="60" width="40" height="50" fill="#00ff00" opacity="0.8"/>
  <rect x="85" y="65" width="30" height="30" fill="#00ffff" opacity="0.6"/>
  <rect x="90" y="70" width="20" height="20" fill="#ff1493" opacity="0.7"/>
  
  <!-- Eyes -->
  <rect x="95" y="75" width="4" height="4" fill="#000"/>
  <rect x="105" y="75" width="4" height="4" fill="#000"/>
  
  <!-- Retro grid pattern -->
  <defs>
    <pattern id="grid" width="20" height="20" patternUnits="userSpaceOnUse">
      <path d="M 20 0 L 0 0 0 20" fill="none" stroke="#00ff00" stroke-width="0.5" opacity="0.3"/>
    </pattern>
  </defs>
  <rect width="200" height="200" fill="url(#grid)"/>
  
  <!-- Profile text -->
  <text x="100" y="140" font-family="Courier New, monospace" font-size="12" fill="#00ff00" text-anchor="middle" font-weight="bold">ROHAN SURANA</text>
  <text x="100" y="155" font-family="Courier New, monospace" font-size="10" fill="#00ffff" text-anchor="middle">AI RESEARCH ENGINEER</text>
  <text x="100" y="170" font-family="Courier New, monospace" font-size="8" fill="#ffff00" text-anchor="middle">DELL TECHNOLOGIES</text>
  
  <!-- Retro border effect -->
  <rect x="5" y="5" width="190" height="190" fill="none" stroke="#ff1493" stroke-width="2" opacity="0.8"/>
  <rect x="10" y="10" width="180" height="180" fill="none" stroke="#00ffff" stroke-width="1" opacity="0.6"/>
</svg>